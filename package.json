    "scripts": {
        "build:testdir": "cross-env mkdirp build && cross-env mkdirp build/testData",
        "build:dir": "cross-env mkdirp build && cross-env mkdirp build/css && cross-env mkdirp build/js && cross-env mkdirp build/fonts && cross-env mkdirp build/images",
        "build:bootstrap": "cross-env xcopy /E /I /Y node_modules\\bootstrap\\dist\\* build",
        "build:jquery": "cross-env xcopy /E /I /Y node_modules\\jquery\\dist\\* build\\js",
        "build:html": "cross-env xcopy /E /I /Y src\\css\\* build\\css && cross-env xcopy /E /I /Y src\\html\\* build",
        "build:testhtml": "cross-env xcopy /E /I /Y src\\html_test\\* build",
        "build:images": "cross-env xcopy /E /I /Y src\\images\\* build\\images && cross-env xcopy /E /I /Y src\\assets\\*.png build\\images",
        "build:popper": "echo Skipping popper - not needed",
        "build:testData": "cross-env xcopy /E /I /Y test-data\\*.json build\\testData",
        "build:manifest": "cross-env copy src\\manifest.json build\\manifest.json",
        "build:sw": "cross-env copy src\\service-worker.js build\\js\\service-worker.js",
        "build-test-assets": "npm run build:testdir && npm run build:testhtml && npm run build:testData",
        "build-assets": "npm run build:dir && npm run build:jquery && npm run build:bootstrap && npm run build:html && npm run build:images && npm run build:manifest && npm run build:sw",
        "build-dev": "npm run build-assets && npx webpack --mode development",
        "build-prod": "npm run build-assets && npx webpack --env prod --mode production",
        "build-test": "npm run build-test-assets",
        "clean": "cross-env rmdir /S /Q build",
        "lint": "eslint .",
        "lintfix": "eslint --fix .",
        "watch": "npx webpack --mode development --watch",
        "watch-prod": "npx webpack --env prod --mode production --watch",
        "webpack-analyze": "npx webpack --env prod --mode production --profile --json > stats.json && npx webpack-bundle-analyzer stats.json ./build/js",
        "test": "jest",
        "storybook": "start-storybook -s ./build -p 6006",
        "build-storybook": "build-storybook -o ./doc/site/dist/storybook"
    },
    "standard": {
        "globals": [
            "chrome",
            "performance"
        ],
        "ignore": [
            "webpack.config.js"
        ],
        "parser": "babel-eslint"
    }
}
